set initial num_zones=20000
set initial model=uniform
set physics rk_order=2
set physics bc_lo=inflow
set physics bc_hi=inflow
set physics inner_radius=1.0
set physics outer_radius=2.0
set physics v_inner=0.5
set physics v_outer=0.5
set output=binary
repeat 0.01 t show iteration
select products
show all
init
t -> 0.1
write products
show profiler
stop
